{
	"autoclose": false,
	"backup": false,
	"colorscheme": "ckGray28",
	"diff": false,
	"fastdirty": true,
	"ftoptions": false,
	"hlsearch": true,
	"hltaberrors": true,
	"hltrailingws": true,
	"matchbracestyle": "highlight",
	"pageoverlap": 4,
	"rmtrailingws": true,
	"scrollmargin": 0,
	"scrollspeed": 1,
	"softwrap": true,
	"statusformatl": "$(filename) $(modified) $(status.paste) $(line):$(col)",
	"statusformatr": "filetype: $(opt:filetype) | $(opt:fileformat) | $(opt:encoding)",
	"tabhighlight": true
}
