{
    "autoclose": false,
    "backup": false,
    "colorscheme": "zzChromaKeys2",
    "diff": false,
    "fastdirty": true,
    "ftoptions": false,
    "hlsearch": true,
    "hltaberrors": true,
    "hltrailingws": true,
    "linter": false,
    "matchbracestyle": "highlight",
    "mouse": false,
    "pageoverlap": 4,
    "rmtrailingws": true,
    "scrollmargin": 0,
    "scrollspeed": 1,
    "softwrap": true,
    "statusformatl": "$(filename) $(modified) $(status.paste) $(line):$(col)",
    "statusformatr": "filetype: $(opt:filetype) | $(opt:fileformat) | $(opt:encoding)",
    "tabhighlight": true
}
